{
  "type": "object",
  "additionalProperties": false,
  "required": [
    "description",
    "registry"
  ],
  "properties": {
    "description": {
      "type": "object",
      "properties": {
        "application": {
          "type": "string"
        },
        "application.version": {
          "type": "string"
        },
        "dubbo": {
          "type": "string"
        },
        "pid": {
          "type": "number"
        },
        "version": {
          "type": "string"
        }
      }
    },
    "dubbo": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "timeout": {
          "type": "number"
        },
        "keepAlive": {
          "type": "boolean"
        },
        "protocol": {
          "enum": [
            "jsonrpc"
          ]
        }
      }
    },
    "registry": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "url"
      ],
      "properties": {
        "url": {
          "type": "string"
        },
        "options": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "sessionTimeout": {
              "type": "number"
            },
            "spinDelay": {
              "type": "number"
            },
            "retries": {
              "type": "number"
            }
          }
        }
      }
    },
    "loadBalance": {
      "type": "string",
      "enum": [
        "round",
        "random"
      ]
    },
    "debug": {
      "type": "boolean"
    }
  }
}
